{
  "name": "nativescript-globalevents-free",
  "version": "1.2.3",
  "description": "A NativeScript plugin to deal with global page events. Since the original plugin requires a subscription now, this plugin is my attempt to maintain it to the point where it does not break.",
  "main": "globalevents",
  "nativescript": {
	"platforms": {
		"android": "6.0.0",
		"ios": "6.0.0"
	},
	"plugin": {
	  "nan": "true",
	  "pan": "true",
	  "core3": "true",
	  "vue": "true",
	  "category": "Processing"
	}
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibnYusrat/nativescript-globalevents"
  },
  "keywords": [
    "NativeScript", "page events", "global events", "ios", "android"
  ],
  "author": {
    "name": "Nathanael Anderson",
    "email": "nathan@master-technology.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://git.proplugins.org/proplugins/nativescript-globalevents/issues"
  },
  "homepage": "https://github.com/ibnYusrat/nativescript-globalevents",
  "readmeFilename": "README.md",
  "scripts": {
    "postinstall": "node postinstall.js || echo \"ignore\""
  }
}
